<div class="card">
    <div class="card-header-primary">
        <div class="card-head">
            <h2>Search Space for Orgs</h2>
        </div>
    </div>
    <div class="card-content">
        <form asp-action="SearchBuilding">
            <div class="form-group">
                <label class="control-label">Building Name (or Part of Building Name)</label>
                <input class="form-control" id="id" name="id" />
            </div>
            <div class="form-group">
                <input type="submit" value="Search" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
