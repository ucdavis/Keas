
@{
    ViewData["Title"] = "Upload CSV";
}

        <div class="card keys-color">
            <div class="card-header-keys">
                <div class="card-head">
                    <h2>Upload Keys</h2>
                </div>
            </div>
            <div class="card-content">
                <a asp-action="UploadKeys" class="btn btn-link">Upload Keys from CSV <i class="fas fa-chevron-right fa-sm" aria-hidden="true"></i></a>
            </div>
        </div>
        <div class="card equipment-color">
            <div class="card-header-equipment">
                <div class="card-head">
                    <h2>Upload Equipment</h2>
                </div>
            </div>
            <div class="card-content">
                <a asp-controller="TeamAdmin" asp-action="UploadEquipment" class="btn btn-link">Upload Equipment from CSV <i class="fas fa-chevron-right fa-sm" aria-hidden="true"></i></a>
            </div>
        </div>
        <div class="card people-color">
            <div class="card-header-people">
                <div class="card-head">
                    <h2>Upload People</h2>
                </div>
            </div>
            <div class="card-content">
                <a asp-controller="PersonAdmin" asp-action="UploadPeople" class="btn btn-link">Load people from CSV <i class="fas fa-chevron-right fa-sm" aria-hidden="true"></i></a>
            </div>
        </div>



